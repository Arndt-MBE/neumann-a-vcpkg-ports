{
  "name": "libtorch",
  "version": "2.1.2",
  "description": "Tensors and Dynamic neural networks in Python with strong GPU acceleration",
  "homepage": "https://pytorch.org/",
  "license": null,
  "supports": "(windows & !static) | osx | linux",
  "dependencies": [
    "blas",
    "cpuinfo",
    "eigen3",
    "flatbuffers",
    {
      "name": "flatbuffers",
      "host": true
    },
    "fmt",
    "foxi",
    "foxi",
    "fp16",
    "gemmlowp",
    "gflags",
    "glog",
    "lapack",
    "lmdb",
    "magma",
    "mimalloc",
    "numpy",
    "onnx",
    "onnx-optimizer",
    "opencl",
    "pocketfft",
    "protobuf",
    {
      "name": "protobuf",
      "host": true
    },
    "pthreadpool",
    "pybind11",
    "python-pyyaml",
    "python3",
    "sleef",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "vcpkg-get-python-packages",
      "host": true
    }
  ],
  "default-features": [
    "fftw3",
    "opencv",
    "python",
    "xnnpack",
    "zstd"
  ],
  "features": {
    "cuda": {
      "description": "Build with CUDA GPU backend",
      "dependencies": [
        "cuda",
        "cudnn",
        "cusparselt"
      ]
    },
    "dist": {
      "description": "Use distributed with MPI, Gloo, libuv, TensorPipe",
      "dependencies": [
        {
          "name": "gloo",
          "platform": "linux"
        },
        {
          "name": "libuv",
          "platform": "windows | osx"
        },
        {
          "name": "mpi",
          "platform": "linux | osx"
        },
        {
          "name": "tensorpipe",
          "platform": "linux | osx"
        }
      ]
    },
    "fftw3": {
      "description": "Build with fftw3",
      "dependencies": [
        "fftw3"
      ]
    },
    "leveldb": {
      "description": "Build with LevelDB",
      "dependencies": [
        "leveldb",
        "snappy"
      ]
    },
    "llvm": {
      "description": "Build with LLVM",
      "dependencies": [
        "llvm"
      ]
    },
    "nnpack": {
      "description": "Build with NNPack",
      "supports": "linux | osx",
      "dependencies": [
        "nnpack"
      ]
    },
    "opencv": {
      "description": "Build with OpenCV 3.x",
      "dependencies": [
        "opencv"
      ]
    },
    "python": {
      "description": "Build with python",
      "dependencies": [
        "numpy",
        "python-astunparse",
        "python-setuptools",
        "python-six",
        "python-sympy",
        "python-typing-extensions",
        "python3",
        "vcpkg-python-scripts"
      ]
    },
    "vulkan": {
      "description": "Build with Vulkan GPU backend",
      "dependencies": [
        "vulkan",
        "vulkan-loader",
        "vulkan-memory-allocator"
      ]
    },
    "xnnpack": {
      "description": "Build with XNNPack",
      "dependencies": [
        "xnnpack"
      ]
    },
    "zstd": {
      "description": "Build with ZSTD",
      "dependencies": [
        "zstd"
      ]
    }
  }
}
