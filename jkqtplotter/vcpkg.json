{
  "name": "jkqtplotter",
  "version": "4.0.0",
  "description": "A Qt Plotting Library",
  "license": "LGPL-2.1-or-later",
  "dependencies": [
    "cimg",
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "gui",
        "jpeg",
        "png",
        "widgets"
      ]
    },
    "qtsvg",
    {
      "name": "vcpkg-cmake",
      "host": true,
      "default-features": false
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true,
      "default-features": false
    }
  ]
}
