{
  "name": "salome-gui",
  "version": "9.10.0",
  "description": "SALOME GUI module",
  "homepage": "https://www.salome-platform.org",
  "license": "GPL-2.0-or-later",
  "dependencies": [
    "salome-configuration",
    "salome-kernel",
    "opencascade",
    {
      "name": "paraview",
      "default-features": false,
      "features": ["tools" , "python"]
    },
    {
      "name": "python3",
      "host": true
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "vcpkg-get-python-packages",
      "host": true
    },
    {
      "name": "qtwebengine",
      "default-features": false
    },
    {
      "name": "qtbase",
      "default-features": false,
      "features": [ "gui" , "widgets", "testlib", "network", "opengl" ]
    },
    {
      "name": "qttools",
      "default-features": false,
      "features": [ "linguist" ]
    },
    "pyside",
    "qwt"
  ]
}
