{
  "name": "salome-gui",
  "version": "9.10.0",
  "description": "SALOME GUI module",
  "homepage": "https://www.salome-platform.org",
  "license": "GPL-2.0-or-later",
  "dependencies": [
    "opencascade",
    {
      "name": "paraview",
      "default-features": false,
      "features": [
        "python",
        "tools"
      ]
    },
    "pyside",
    {
      "name": "python3",
      "host": true
    },
    {
      "name": "qtbase",
      "default-features": false,
      "features": [
        "gui",
        "network",
        "opengl",
        "testlib",
        "widgets"
      ]
    },
    {
      "name": "qttools",
      "default-features": false,
      "features": [
        "linguist"
      ]
    },
    {
      "name": "qtwebengine",
      "default-features": false
    },
    "qwt",
    "salome-configuration",
    "salome-kernel",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    {
      "name": "vcpkg-get-python-packages",
      "host": true
    }
  ]
}
