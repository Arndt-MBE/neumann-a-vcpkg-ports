{
  "name": "freecad",
  "version": "0.20.2",
  "description": "FreeCAD, a free and opensource multiplatform 3D parametric modeler. ",
  "homepage": "www.freecad.org",
  "license": "GPL-2.0-or-later",
  "dependencies": [
    "boost-date-time",
    "boost-filesystem",
    "boost-program-options",
    "boost-regex",
    "boost-system",
    "boost-thread",
    "coin",
    "eigen3",
    "freetype",
    "matplotlib-cpp",
    "med-fichier",
    "netgen",
    {
      "name": "opencascade",
      "default-features": false
    },
    {
      "name": "opencv",
      "default-features": false
    },
    "opengl",
    "orocos-kdl",
    {
      "name": "pcl",
      "default-features": false,
      "features": [
        "qt"
      ]
    },
    "python3",
    "qtsvg",
    {
      "name": "qtwebengine",
      "default-features": false
    },
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    },
    "xerces-c"
  ]
}
